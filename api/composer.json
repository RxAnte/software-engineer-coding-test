{
    "name": "software-engineering/coding-test",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "buzzingpixel/container": "^1.1",
        "buzzingpixel/valueobject-ext": "^1.0",
        "funeralzone/valueobjects": "^0.5.0",
        "lcobucci/clock": "^3.1",
        "mnapoli/silly": "^1.8",
        "psr/clock": "^1.0",
        "psr/container": "^2.0",
        "ramsey/uuid": "^4.7",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.12",
        "spatie/php-cloneable": "^1.0"
    },
    "require-dev": {
        "buzzingpixel/symfony-var-dumper-decorator": "^1.0",
        "doctrine/coding-standard": "^11.0.0",
        "filp/whoops": "^2.14",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-strict-rules": "^1.5",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/var-dumper": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Config\\": "config",
            "SoftwareEngineering\\": "src"
        }
    },
    "config": {
        "bin-compat": "full",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
